{
  "name": "trading-platform-simulator",
  "version": "1.0.0",
  "description": "A simulated trading platform for practicing stock and asset trades with virtual money.",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "migrate": "tsx ./node_modules/.bin/knex migrate:latest --knexfile knexfile.ts",
    "rollback": "tsx ./node_modules/.bin/knex migrate:rollback --knexfile knexfile.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "node",
    "typescript",
    "trading",
    "knex",
    "postgres",
    "portfolio"
  ],
  "author": "Matthew Kropp",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "knex": "^3.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsx": "^4.19.4",
    "typescript": "^5.4.5"
  }
}
